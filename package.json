{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && yarn start",
    "server": "nodemon server.js",
    "lofty": "npx lofty-take-home-server",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\" \"yarn lofty\"",
    "bootstrap": "cd ./client && yarn install && cd .. && yarn install"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "nodemon": "2.0.7",
    "http-proxy-middleware": "1.3.1"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
